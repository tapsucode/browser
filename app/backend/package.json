{
  "name": "antidetect-browser-backend",
  "version": "1.0.0",
  "description": "Backend for Anti-Detect Browser with SQLite storage",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc",
    "db:generate": "drizzle-kit generate",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "db:push": "drizzle-kit push",
    "db:migrate": "ts-node ./db/migrations/run.ts",
    "db:seed": "ts-node src/db/seeds/seed.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "p-limit": "^3.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.16.11",
    "@types/p-limit": "^2.1.0",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
